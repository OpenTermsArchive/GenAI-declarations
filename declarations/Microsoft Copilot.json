      "select": [
        "#page_content"
      ],
      "remove": [
        ".cdx_mkt_brand"
      ],
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://privacy.microsoft.com/en-ie/privacystatement",
      "select": [
        "[role=\"main\"]"
      ],
      "remove": [
        ".expand_collapse",
        ".div_print",
        ".nav_content",
        ".LearnMoreNavigation",
        ".TopNavigation",
        ".CollectingYourInfoRightNav",
        ".gcc_cookies",
        "#dvGroupModules",
        "#dvServicesAgreement"
      ]
    }
  }
}
