{
  "name": "Rephrase.ai",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://www.rephrase.ai/privacy-policy",
      "select": [
        ".heading-pageheader",
        ".policypg"
      ]
    },
    "Terms of Service": {
      "fetch": "https://www.rephrase.ai/terms-of-service",
      "select": [
        ".heading-pageheader",
        ".policypg"
      ]
    },
    "Trackers Policy": {
      "fetch": "https://www.rephrase.ai/cookie-policy",
      "select": [
        ".heading-pageheader",
        ".iub_content"
      ],
      "remove": [
        ".iub_footer > p:nth-child(2)",
        ".iub_footer > p:nth-child(3)"
      ],
      "executeClientScripts": true
    }
  }
}
