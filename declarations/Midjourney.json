{
  "name": "Midjourney",
  "documents": {
    "Terms of Service": {
      "fetch": "https://docs.midjourney.com/docs/terms-of-service",
      "select": [
        ".content_container"
      ],
      "remove": [
        "#lightTheme",
        "#darkTheme",
        "#ai-article-summary-callout",
        "#navigation-container",
        "#right_sidebar"
      ],
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://docs.midjourney.com/docs/privacy-policy",
      "select": [
        ".content_container"
      ],
      "remove": [
        "#navigation-container",
        ".article-action-items:nth-child(2) li"
      ],
      "executeClientScripts": true
    }
  }
}
