{
  "name": "Speechify",
  "documents": {
    "Terms of Service": {
      "fetch": "https://speechify.com/terms",
      "select": "main"
    },
    "Trackers Policy": {
      "fetch": "https://speechify.com/privacy/",
      "select": [
        {
          "startBefore": "#cookie-policy",
          "endAfter": "main"
        }
      ]
    },
    "Privacy Policy": {
      "fetch": "https://speechify.com/privacy/",
      "select": {
        "startBefore": "main",
        "endBefore": "#cookie-policy"
      }
    }
  }
}
